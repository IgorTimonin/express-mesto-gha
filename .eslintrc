{
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id"]
      }
    ],
    "jsx-quotes": [1, "prefer-double"],
    "prettier/prettier": "error"
  },
  "plugins": ["prettier"]
}
